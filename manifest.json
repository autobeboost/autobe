{
  "manifest_version": 3,
  "name": "AutoBe Boost",
  "version": "3.4",
  "description": "Automatizador para o Behance",
  "host_permissions": ["https://www.behance.net/*"],
  "action": {
    "default_icon": "images/icon48.png",
    "default_popup": "index.html",
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "cookies",
    "alarms",
    "tabs",
    "notifications"
  ]
}
